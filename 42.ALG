Algoritmo "42"
// Lógica de Programação
// Robson D. Preuss
// 07/06/2021
// Gh Branding
Var
   Contador, Numero: Inteiro

Inicio
   Contador <- 0
   
   Escreval("Digite um Valor? ")
   Leia(Numero)
   Enquanto Contador <= Numero faca
      Escreval(Contador)
      Contador <- Contador + 1
   Fimenquanto
   Escreval("Acabou!")

Fimalgoritmo